doctype html
html
  head
    meta(charset='utf-8')
    title #{title}
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(href='/stylesheets/bootstrap.min.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/dataTables.bootstrap.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/dataTables.tableTools.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/editor.bootstrap.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/layout.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/form.css', rel='stylesheet', type='text/css', media='screen')
    link(href='/stylesheets/style.css', rel='stylesheet', type='text/css', media='screen')
    // DataTables CSS
    link(rel='stylesheet', type='text/css', href='//cdn.datatables.net/1.10.6/css/jquery.dataTables.css')
    link(href='/stylesheets/jquery.dataTables.min.css', rel='stylesheet', type='text/css', media='screen')
    script(src='/javascripts/jquery-2.1.3.min.js', type='application/javascript')
    script(src='/javascripts/jquery.easing.1.3.js', type='application/javascript')
    script(src='/javascripts/menu-slider.js', type='application/javascript')
    script(src='/javascripts/ajax_register.js', type='application/javascript')
    script(src='/javascripts/jquery.dataTables.min.js', type='application/javascript')
    // jQuery
    script(type='text/javascript', charset='utf8', src='//code.jquery.com/jquery-1.10.2.min.js')
    // DataTables
    script(type='text/javascript', charset='utf8', src='//cdn.datatables.net/1.10.6/js/jquery.dataTables.js')
    script.
      $(document).ready(function(){
      $('#table_id').DataTable();
      });
    style.
     body { font-size: 140%; }
  body#datawid
    .container
      header
        a(href='/dashboard')
          img(src='/images/media/socket-app-logo.png', alt='socket-logo')
        br
        nav(style='text-align: center; min-width: 1270px')
          ul
            li
              a(href='/admin/updateterms') TERMS
            li
              a(href='/admin/data') USER DATA
        // Nav menu for smaller devices
        .slideOut
          a(href='', title='Navigation Panel') ➜
          ul
            li
              a(href='/admin/updateterms') TERMS
            li
              a(href='/admin/data') USER DATA
        // 
      // end .header
      .content
        figure
          a(href='/admin/signout')
            img#singn-out(src='https://cdn2.iconfinder.com/data/icons/picons-essentials/57/logout-512.png', width='50px', alt='sign-out-icon')
        div#datatbl
          table#table_id.display
            thead
              tr
                th First Name
                th Last Name
                th User Type
                th Gender
                th Email ID
                th Password
                th Vehicle Name
                th Model Name
                th Date
                th Vehicle Type
                th Licence Number
            tbody
              tr
                td Row 1 Data 1
                td Row 1 Data 2
                td Row 1 Data 3
                td Row 1 Data 4
                td Row 1 Data 5
                td Row 1 Data 6
                td Row 1 Data 7
                td Row 1 Data 8
                td Row 1 Data 9
                td Row 1 Data 10
                td Row 1 Data 11
              tr
                td Row 2 Data 1
                td Row 2 Data 2
                td Row 2 Data 3
                td Row 2 Data 4
                td Row 2 Data 5
                td Row 2 Data 6
                td Row 2 Data 7
                td Row 2 Data 8
                td Row 2 Data 9
                td Row 2 Data 10
                td Row 2 Data 11
      
      // end .container
